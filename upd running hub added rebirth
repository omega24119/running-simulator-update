local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "key system", HidePremium = false, SaveConfig = true, IntroText = "key system"})

-- Notification
OrionLib:MakeNotification({
  Name = "Logged in!",
  Content = "You are logged in as " .. Player.Name .. ".",
  Image = "rbxassetid://4483345998",
  Time = 5
})

-- Values
getgenv().key = "ALLUNIVERSEONTOP241109"
getgenv().keyinput = "string"

-- Functions
function makescript()
  destroy()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/omega24119/running-sim/main/running%20sim",true))()
end

function keynoti()
  OrionLib:MakeNotification({
    Name = "correct key",
    Content = "access granted",
    Image = "rbxassetid://4483345998",
    Time = 5
  })
end

function nokeynoti()
  OrionLib:MakeNotification({
    Name = "incorrect key",
    Content = "no access granted incorrect key",
    Image = "rbxassetid://4483345998",
    Time = 5
  })
end

function destroy()
  game:GetService("CoreGui").Orion:Destroy()
end

-- Tabs
local Tab = Window:MakeTab({
  Name = "Key",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

-- Textbox
Tab:AddTextbox({
  Name = "Enter key",
  Default = "Key here",
  TextDisappear = true,
  Callback = function(Value)
    getgenv().keyinput = Value
  end
})

-- Buttons
Tab:AddButton({
  Name = "check key",
  Callback = function()
    if getgenv().keyinput == getgenv().key then
      makescript()
      keynoti()
    else
      nokeynoti()
    end
  end
})

local Discord = Tab:AddSection({Name="Join Our Discord To Get Key"});

Discord:AddButton({
  Name = "Copy Discord Link",
  Callback = function()
      setclipboard("https://discord.gg/e3zTG6KT")
      print("Discord link copied to clipboard")
  end    
})
